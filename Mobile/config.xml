<?xml version='1.0' encoding='utf-8'?>
<widget id="io.jok.pitching" version="0.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    
    <name>Pitching</name>
    
    <description>
        Inspired by Game Jam Pitching process, in Tbilisi, Georgia.
    </description>
    
    <author email="contact@jok.io" href="http://jok.io">
        Jok Entertainers
    </author>
    
    <content src="index.html" />
    
    <access origin="*" />
    
    <preference name="UIWebViewDecelerationSpeed" value="fast" />
    <preference name="Fullscreen" value="true" />
    <preference name="Orientation" value="landscape" />
    <preference name="DisallowOverscroll" value="true"/>
    <preference name="HideKeyboardFormAccessoryBar" value="true"/>

</widget>
