<?xml version='1.0' encoding='utf-8'?>
<widget id="io.jok.pitching" version="1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Pitching</name>
    <description>
        Inspired by Game Jam Pitching process, in Tbilisi, Georgia.
    </description>
    <author email="contact@jok.io" href="http://jok.io">
        Jok Entertainers
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="UIWebViewDecelerationSpeed" value="fast" />
    <preference name="Fullscreen" value="true" />
    <preference name="Orientation" value="landscape" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="HideKeyboardFormAccessoryBar" value="true" />
    <feature name="Device">
        <param name="wp-package" value="Device" />
    </feature>
    <feature name="Accelerometer">
        <param name="wp-package" value="Accelerometer" />
    </feature>
    <feature name="Notification">
        <param name="wp-package" value="Notification" />
    </feature>
    <feature name="InAppBrowser">
        <param name="wp-package" value="InAppBrowser" />
    </feature>
    <feature name="Media">
        <param name="wp-package" value="Media" />
    </feature>
    <feature name="Vibration">
        <param name="wp-package" value="Vibration" />
    </feature>
</widget>
